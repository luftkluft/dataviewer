{
  "name": "dataviewer",
  "version": "1.0.0",
  "description": "DataViewer from luftkluft",
  "main": "dist/app/main.js",
  "email": "luftkluft@gmail.com",
  "homepage": "https://github.com/luftkluft/dataviewer",
  "scripts": {
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "transpile": "tsc",
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "electron:linux": "electron-builder --linux",
    "electron:windows": "electron-builder --win"
  },
  "author": "luftkluft<luftkluft@gmail.com>",
  "license": "MIT",
  "build": {
    "productName": "DataViewer",
    "icon": "./app/public/icons/app-256x256.png",
    "appId": "luftkluft.dataviewer",
    "linux": {
      "target": "deb",
      "icon": "./app/public/icons/app-256x256.png",
      "desktop": "./app/public/icons/app-256x256.png",
      "category": "Utility"
    },
    "directories": {
      "output": "soft"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./app/public/icons/app-256x256.png",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "uninstallDisplayName": "DataViewer",
      "license": "LICENSE",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@types/electron": "1.6.10",
    "@types/node": "14.17.29",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "electron": "13.5.2",
    "electron-builder": "22.13.1",
    "eslint": "8.1.0",
    "prettier": "2.4.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "app-root-path": "3.0.0",
    "bootstrap": "5.1.3",
    "electron-alert": "0.1.19",
    "electron-ejs": "1.2.1",
    "jquery": "3.6.0"
  }
}
