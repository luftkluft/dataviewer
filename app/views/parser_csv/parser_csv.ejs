<!DOCTYPE html>
<html lang="en">

  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="../../../node_modules/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="./parser_csv.css">
    <script src="../../../node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="../../../node_modules/jquery/dist/jquery.min.js"></script>
    <link rel="shortcut icon" href="<%= shortcutIcon %>">
  </head>

  <body>
    <form class='parser-form'>
      <div>
        <div>
          <label><%= I18n.t('target_file_name') %>:</label>
        </div>
        <div>
          <input type='text' class='target-file-name-field' />
        </div>
        <div class="field">
          <label><%= I18n.t('head_row') %>:</label>
          <input type='text' class='head-row-field' list="head-row" />
          <datalist id="head-row">
            <option value="0">
            <option value="1">
            <option value="2">
            <option value="3">
          </datalist>
        </div>
        <div class="field">
          <label><%= I18n.t('end_row') %>:</label>
          <input type='text' class='end-row-field' list="end-row" />
          <datalist id="end-row">
            <option value="\n">
            <option value="\r">
          </datalist>
        </div>
        <div class="field">
          <label><%= I18n.t('delemiter') %>:</label>
          <input type='text' class='delemiter-field' list="delemiter" />
          <datalist id="delemiter">
            <option value="'">
            <option value=":">
          </datalist>
        </div>
        <div class="field">
          <label><%= I18n.t('rows') %>:</label>
          <input type='text' class='rows-field' list="rows" />
          <datalist id="rows">
            <option value="0">
          </datalist>
        </div>
        <div class="field">
          <label><%= I18n.t('columns') %>:</label>
          <input type='text' class='columns-field' list="columns" />
          <datalist id="columns">
            <option value="0">
          </datalist>
        </div>
        <div class="field">
          <button type='submit' class='set-parser-params-btn'>TODO</button>
        </div>
      </div>
    </form>
    <script>const exports = { "__esModule": true }</script>
    <script src="./parserCSV.js"></script>
  </body>

</html>
